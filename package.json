{
  "name": "ethereum-transaction-tracer",
  "version": "1.0.0",
  "description": "🐾 Simple REST tracer for Ethereum transactions",
  "keywords": [
    "ethereum",
    "transaction",
    "tracer",
    "parity",
    "rest"
  ],
  "homepage": "https://github.com/autonomoussoftware/ethereum-transaction-tracer#readme",
  "bugs": {
    "url": "https://github.com/autonomoussoftware/ethereum-transaction-tracer/issues"
  },
  "license": "MIT",
  "author": "Ignacio Anaya <ignacio@bloq.com>",
  "main": "server.js",
  "repository": "autonomoussoftware/ethereum-transaction-tracer",
  "scripts": {
    "check": "dependency-check . && dependency-check . --unused --no-dev",
