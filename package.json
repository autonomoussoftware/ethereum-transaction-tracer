    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-jsdoc": "3.7.0",
    "eslint-plugin-mocha": "5.0.0",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-prefer-arrow": "1.1.1",
    "eslint-plugin-promise": "3.7.0",
    "eslint-plugin-standard": "3.1.0",
    "husky": "0.14.3",
    "nodemon": "1.17.4"
