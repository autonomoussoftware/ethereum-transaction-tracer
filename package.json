    "winston": "2.4.2",
    "winston-child-logger": "0.0.1",
    "winston-papertrail": "1.0.5",
    "winston-sentry-transport": "1.0.0"
  },
  "devDependencies": {
    "dependency-check": "3.1.0",
    "eslint": "4.19.1",
    "eslint-config-bloq": "1.2.2",
